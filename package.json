{
  "name": "compass-financial",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "prebuild:clean:android": "npx expo prebuild --clean --platform android",
    "prebuild:clean:ios": "npx expo prebuild --clean --platform ios",
    "prebuild:clean:all": "npx expo prebuild --clean",
    "android:device": "npx expo prebuild --clean --platform android && npx expo run:android --device",
    "ios:device": "npx expo prebuild --clean --platform ios && npx expo run:ios --device",
    "build:android:production": "eas build --platform android --profile production",
    "build:ios:production": "eas build --platform ios --profile production",
    "build:all:production": "eas build --platform all --profile production"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@react-navigation/native": "^7.1.8",
    "expo": "~54.0.29",
    "expo-constants": "~18.0.12",
    "expo-dev-client": "^6.0.20",
    "expo-font": "~14.0.10",
    "expo-linking": "~8.0.10",
    "expo-router": "~6.0.19",
    "expo-splash-screen": "~31.0.12",
    "expo-status-bar": "~3.0.9",
    "expo-web-browser": "~15.0.10",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "^2.29.1",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "^15.15.1",
    "react-native-wagmi-charts": "^2.8.3",
    "react-native-web": "~0.21.0",
    "react-native-worklets": "0.5.1"
  },
  "devDependencies": {
    "@types/react": "~19.1.0",
    "react-test-renderer": "19.1.0",
    "typescript": "~5.9.2"
  },
  "private": true
}
